{
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "24": "icons/icon24.png",
            "32": "icons/icon32.png"
        },
        "default_title": "DataGalaxy Chromium Extension"
    },
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "PAGE_TO_OVERRIDE": "https://dev-uat.datagalaxy.com/"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["content.css"],
            "js": ["js/popup.js"]
        },
        {
            "matches": ["<all_urls>"],
            "js": ["pendo.js"]
        }
    ],
    "default_locale": "en",
    "description": "__MSG_appDesc__",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "permissions": ["webRequest", "activeTab", "scripting", "storage", "declarativeNetRequest"],
    "version": "1.0.31",
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "priority": 1,
                "path": "rules.json",
                "enabled": true
            }
        ]
    }
}
